    "name" : "Dictionary to Xml Task",
    "clazz" : "core.DictionaryToXMLTask",
    "enabled" : true,
    "desc" : "Task used to convert a dictionary entry to xml file.",
    "refId" : "a7958eba-812f-40bd-b0db-267cdea96427",
    "stopOnError" : true,
