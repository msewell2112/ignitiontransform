  }, {
    "id" : "2aab8715-0fa4-40bf-9b60-c1fd6adefade",
    "ordinal" : 4,
    "taskParameters" : [ {
      "id" : "ccb4af80-31f8-4b6b-aa0b-c926da6810b2",
      "ordinal" : 1,
      "value" : "var inDebug = dict.inDebug; \n$includeScript('global');\n\ntry {\n    var productIterator = ICT.MAP.ITERATOR('SFCCProductMap');\n    var categoryIterator = ICT.MAP.ITERATOR('SFCCCategoryMap');\n    var root = new java.util.HashMap();\n    \n    var products = new java.util.ArrayList();\n    var glCategories = new java.util.ArrayList();\n\n    while (productIterator.hasNext()) {\n      var productRecord = productIterator.next();\n      \n      var product = new java.util.HashMap();\n      \n      product.put(\"ExternalId\", productRecord.product_id);\n      product.put(\"Name\", productRecord.product_name);\n      product.put(\"Description\", productRecord.product_long_description);\n      product.put(\"CategoryExternalId\", productRecord.product_category);\n      product.put(\"ProductPageUrl\", productRecord.product_url);\n      product.put(\"ImageUrl\", productRecord.product_image_link);\n      \n      var manufacturerPartNumbers = new java.util.ArrayList();\n      var manufacturerPartNumber = new java.util.HashMap();\n      manufacturerPartNumber.put(\"ManufacturerPartNumber\", productRecord.product_manufacturer_sku);\n      manufacturerPartNumbers.add(manufacturerPartNumber);\n      product.put(\"ManufacturerPartNumbers\", manufacturerPartNumbers);\n      \n      var recordMap = new java.util.HashMap();\n      recordMap.put(\"Product\", product);\n      \n      products.add(recordMap);\n    }\n\n    while (categoryIterator.hasNext()) {\n \n      var categoryRecord = categoryIterator.next();\n    \n      var category = new java.util.HashMap();\n      category.put(\"ExternalId\", categoryRecord.cat_id);\n      category.put(\"Name\", categoryRecord.cat_name);\n      category.put(\"CategoryPageUrl\", categoryRecord.cat_url);\n      \n      var recordMap = new java.util.HashMap();\n      recordMap.put(\"Category\", category);\n      \n      glCategories.add(recordMap);\n\t}\n\n\tvar sdf = new java.text.SimpleDateFormat(\"yyyy-MM-dd'T'HH:mm:ss\");\n    root.put(\"Feed\",\"\");\n    root.put(\"Feed@xmlns\", \"http://www.bazaarvoice.com/xs/PRR/ProductFeed/5.2\");\n    root.put(\"Feed@name\", \"ignitiontransform-com\");\n    root.put(\"Feed@incremental\", \"false\");\n    root.put(\"Feed@extractDate\", sdf.format(new java.util.Date()));\n    root.put(\"Feed@generator\", \"Ignition Transform\");\n\n\tvar feed = new java.util.HashMap();\n    feed.put(\"Categories\", glCategories);\n    feed.put(\"Products\", products);\n\n    root.put(\"Feed\", feed);\n    \n    dict.put(\"Root\", root);\n} catch (err) {\n    var errMessage = 'An error occurred while trying to process inventory file Exception: ' + err;\n    ICT.TASK_ERROR('Demo -> Inventory File -> Error', errMessage);    \n}",
      "desc" : "Script to execute",
      "name" : "script",
      "valueType" : 2,
      "secret" : false,
      "refId" : "61734d1c-1ff6-4a2b-a251-00f3bd76b9ed",
