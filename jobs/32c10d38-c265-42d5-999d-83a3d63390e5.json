      "ordinal" : 1,
      "value" : "var inDebug = dict.inDebug; \n$includeScript('global');\n$includeScript('kibo');\ntry {    \n    var KiboCatalog = KIBO.GetCatalog();    \n    dict.put(\"root\", _generateFeed(KiboCatalog, function(record, row) {\n        // Here is where you can modify the object graph that was build up in the         \n        // internal function _generateFeed.  For example, I could add a custom        \n        // attribute by...        \n        //row.put('Customfield', 'My Value');        \n        return row;    \n        \n    }));\n} catch (err) {    \n    var errMessage = 'An error occurred while trying to generate olapic file  Exception: ' + err;    \n    ICT.TASK_ERROR('Olapic -> Error', errMessage);    \n    \n}\n\n//#region PRIVATE\n/** \n * This is meant to be a private function.  Do not modify.  Any modification \n * should be done in the processRow function passed into this function \n */\nfunction _generateFeed(collection, processRow) {   \n    var newRecords = [];\n    collection.forEach(function(value) {\n        var record = {\n            'Name': value.name,\n            'Price': value.price,\n            'ManufacturerPartNumber': value.partNumber,\n            'Available': value.availability,\n            'ImageUrl': value.imageUrl,\n            'ExternalId': value.productID\n        };\n        newRecords[newRecords.length] = processRow(value, record);\n    });\n    return {\n        'records': newRecords\n    };\n}",
